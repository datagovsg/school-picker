{
  "name": "datagovsg-school-picker",
  "version": "1.0.0",
  "description": "A data.gov.sg opensource project",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run start-server",
    "dev": "npm run watch-server & npm run dev-server",
    "start-server": "node dist/server.js",
    "watch-server": "babel-watch src/server.js",
    "dev-server": "BABEL_ENV=webpack2 webpack-dev-server --config ./webpack.config.js --colors --progress",
    "build": "npm run babel && npm run webpack",
    "babel": "mkdir -p dist/ && babel src/server.js -o dist/server.js && babel src/helpers/ -d dist/helpers/",
    "webpack": "BABEL_ENV=webpack2 webpack --config ./webpack.prod.config.js --colors --progress",
    "deploy": "docker build -t yongjun21/datagovsg-school-picker . && heroku container:push web",
    "test": "standard src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongjun21/datagovsg-school-picker.git"
  },
  "keywords": [
    "schools",
    "MOE",
    "Singapore"
  ],
  "author": "Yong Jun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yongjun21/datagovsg-school-picker/issues"
  },
  "homepage": "https://github.com/yongjun21/datagovsg-school-picker#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "webpack2": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-2"
        ]
      }
    }
  },
  "devDependencies": {
    "@turf/buffer": "^3.11.7",
    "@turf/union": "^3.10.5",
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^3.13.1",
    "sass-loader": "^4.1.1",
    "standard": "^8.6.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "https://registry.npmjs.org/vue-template-compiler/-/vue-template-compiler-2.4.2.tgz",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "express-history-api-fallback": "^2.1.0",
    "fastclick": "^1.0.6",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-fetch": "^1.6.3",
    "quasar-framework": "^0.13.6",
    "sg-heatmap": "^2.5.7",
    "vue": "^2.2.6",
    "vue-router": "^2.3.1",
    "vuex": "^2.2.1",
    "whatwg-fetch": "^2.0.2"
  },
  "standard": {
    "globals": [
      "L"
    ]
  }
}
